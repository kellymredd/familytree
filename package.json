{
  "name": "familytree",
  "version": "1.0.0",
  "description": "The REDD Family Tree App",
  "keywords": [
    "react"
  ],
  "scripts": {
    "start": "node app.js",
    "build": "webpack --config webpack.config.js",
    "lint": "eslint --fix --ext .js,.jsx --ignore-path .gitignore ."
  },
  "engines": {
    "node": "16.x",
    "npm": "8.5.0"
  },
  "dependencies": {
    "@emmetio/extract-abbreviation": "0.2.0",
    "axios": "^0.26.1",
    "bcrypt": "^5.0.1",
    "classnames": "2.3.1",
    "cookie-parser": "^1.4.6",
    "css-loader": "^6.7.1",
    "dotenv": "^16.0.0",
    "emmet": "2.3.4",
    "express": "^4.18.1",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "prettier": "2.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "5.2.0",
    "sass-loader": "^13.0.2",
    "sequelize": "^6.21.3",
    "sequelize-cli": "^6.4.1",
    "style-loader": "^3.3.1",
    "uninstall": "^0.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-transform-runtime": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/runtime": "^7.13.8",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "eslint": "^8.24.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^3.2.0",
    "html-webpack-plugin": "^5.5.0",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  },
  "browserslist": [
    "defaults"
  ],
  "prettier": {
    "printWidth": 80,
    "singleQuote": true,
    "tabWidth": 2,
    "useTabs": false
  },
  "eslintConfig": {
    "parser": "@babel/eslint-parser",
    "extends": [
      "eslint:recommended",
      "prettier",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "node": true,
      "es6": true,
      "browser": true
    },
    "rules": {
      "react/prop-types": 0,
      "react/display-name": 0,
      "react/jsx-uses-react": "error",
      "react/jsx-uses-vars": "error",
      "block-scoped-var": 2,
      "comma-style": [
        2,
        "last"
      ],
      "no-undef": 1,
      "no-console": 0,
      "no-unused-vars": 1,
      "semi": [
        2,
        "always"
      ]
    }
  }
}
